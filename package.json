{
  "name": "material-app",
  "version": "2.0.1",
  "description": "Constellateur Frontend",
  "author": "Steve Jin <steve.jin.md@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "dependencies": {
    "@material-ui/core": "^4.12.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^4.0.3",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^6.0.0",
    "lint-staged": ">=10",
    "pm2": "^4.5.4",
    "prettier": "^2.3.0",
    "redux-devtools-extension": "^2.13.8"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix",
    "src/**/*.{js,jsx,json,css}": "prettier --single-quote --write"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "pre-commit": "lint-staged && git add .",
    "lint": "eslint src --fix",
    "format": "prettier --single-quote --write \"src/**/*.{js,jsx,json,css}\""
  }
}
